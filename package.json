{
  "name": "corellium-typescript",
  "version": "1.2.12",
  "description": "A beautiful TypeScript Corellium library.",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsup src",
    "dev": "tsup src --watch",
    "test": "bun test/index.ts",
    "generate-types:core": "npx openapi-typescript@6.7.6 https://app.corellium.com/api/openapi.json -o ./types/corellium.d.ts",
    "generate-types:matrix": "npx openapi-typescript@6.7.6 ./matrix-openapi.yml -o ./types/matrix.d.ts",
    "generate-types": "npm run generate-types:core && npm run generate-types:matrix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haydenbleasel/corellium-typescript.git"
  },
  "keywords": [
    "corellium",
    "typescript",
    "api",
    "client"
  ],
  "author": "Hayden Bleasel <hello@haydenbleasel.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/haydenbleasel/corellium-typescript/issues"
  },
  "homepage": "https://github.com/haydenbleasel/corellium-typescript#readme",
  "devDependencies": {
    "@types/node": "^22.0.2",
    "@types/ws": "^8.5.10",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.6.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "ultracite": "^4.0.4"
  },
  "dependencies": {
    "openapi-fetch": "^0.10.4",
    "ws": "^8.17.0"
  },
  "engines": {
    "node": ">=18"
  }
}
